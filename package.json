{
  "name": "shiki-stream",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@10.4.1",
  "description": "Streaming colorization for Shiki",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/shiki-stream#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/shiki-stream.git"
  },
  "bugs": "https://github.com/antfu/shiki-stream/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    }
  },
  "dependencies": {
    "@shikijs/core": "^2.4.2",
    "@shikijs/types": "^2.4.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.2.1",
    "@antfu/ni": "^23.3.1",
    "@antfu/utils": "^9.1.0",
    "@shikijs/engine-javascript": "^2.4.2",
    "@shikijs/langs": "^2.4.2",
    "@shikijs/themes": "^2.4.2",
    "@types/node": "^22.13.4",
    "bumpp": "^10.0.3",
    "eslint": "^9.20.1",
    "lint-staged": "^15.4.3",
    "pnpm": "^10.4.1",
    "shiki": "^2.4.2",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "vite": "^6.1.0",
    "vitest": "^3.0.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
